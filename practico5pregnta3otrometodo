#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
int main()
{
	int udm,c,d,u,n;
	cout<<"ingrese el numero: ";
	cin>>n;
	if(n<10000)
	{
	u=n%10;
	cout<<"u:"<<u;
	d=(n%100-(n%10))/10;
	cout<<"\nd: "<<d;
	c=((n-(n%100))/100)%10;
	cout<<"\nc: "<<c;
	udm=((n-n%1000)/1000)%10;
	cout<<"\nudm: "<<udm;
	}
else
	cout<<"el numero es mayor a 10000";
_getch();
}
